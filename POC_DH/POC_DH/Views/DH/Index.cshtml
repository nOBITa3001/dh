<div class="row top_tiles">
	<div class="animated flipInY col-lg-3 col-md-3 col-sm-6 col-xs-12">
		<div class="tile-stats">
			<div class="icon">
				<i class="fa fa-users"></i>
			</div>
			<div class="count">@Model.Registers</div>

			<h3>Registers</h3>
			<p>league 2015</p>
		</div>
	</div>
	<div class="animated flipInY col-lg-3 col-md-3 col-sm-6 col-xs-12">
		<div class="tile-stats">
			<div class="icon">
				<i class="fa fa-play-circle-o"></i>
			</div>
			<div class="count">@Model.Activities</div>

			<h3>Activities</h3>
			<p>@Model.DaysProgress days</p>
		</div>
	</div>
	<div class="animated flipInY col-lg-3 col-md-3 col-sm-6 col-xs-12">
		<div class="tile-stats">
			<div class="icon">
				<i class="fa fa-calendar"></i>
			</div>
			<div class="count">@Model.DaysLeft</div>

			<h3>Days left</h3>
			<p>01/09/2015 - 31/10/2015</p>
		</div>
	</div>
	<div class="animated flipInY col-lg-3 col-md-3 col-sm-6 col-xs-12">
		<div class="tile-stats">
			<div class="icon">
				<i class="fa fa-trophy"></i>
			</div>

			@if (@Model.FirstRanking != null)
			{
				<div class="count">@Model.FirstRanking.Name</div>
			}
			else
			{
				<div class="count">-</div>
			}

			<h3>1 st</h3>

			@if (@Model.FirstRanking != null)
			{
				<p>@Model.FirstRanking.Activities activities</p>
			}
			else
			{
				<p>- activities</p>
			}
		</div>
	</div>
</div>

